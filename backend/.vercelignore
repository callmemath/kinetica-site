# Vercel Ignore File
# Files and directories to ignore during Vercel deployment

# Development files
src/
scripts/
*.ts
!vercel.json

# Environment files
.env
.env.*
!.env.example

# Logs
logs/
*.log

# Development database
prisma/dev.db*

# Node modules (Vercel handles this)
node_modules/

# Build artifacts that we don't need
.tsbuildinfo

# IDE files
.vscode/
.idea/

# OS files
.DS_Store
Thumbs.db

# Test files
tests/
__tests__/
*.test.*
*.spec.*

# Documentation (not needed for runtime)
README.md
PRODUCTION.md
VERCEL_DEPLOY.md
